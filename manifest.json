{
    "manifest_version": 2,
    "name": "Youtube Overlay videos Hidder",
    "version": "1.0",
  
    "description": "Hide annoying overlay elements that appear towards the end of youtube videos",

    "icons": {
      "48": "icons/Youtube_no_overlay.png"
    },

    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["extension.js"]
      }
    ],

    "browser_specific_settings": {
        "gecko": {
          "id": "YoutubeNoOverlay@example.com",
          "strict_min_version": "42.0",
        },
        "edge": {
          "browser_action_next_to_addressbar": false
        }
    }
}